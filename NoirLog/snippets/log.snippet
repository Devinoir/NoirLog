<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>log</Title>
			<Shortcut>log</Shortcut>
			<Description>Log snippet</Description>
			<Author>Devinoir</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>text</ID>
					<ToolTip>logText</ToolTip>
					<Default>logText</Default>
				</Literal>
				<Literal>
					<ID>logLevel</ID>
					<ToolTip>LogLevel</ToolTip>
					<Default>NoirLogger.LogLevel.Info</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[logger.Add("$text$", $logLevel$);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
